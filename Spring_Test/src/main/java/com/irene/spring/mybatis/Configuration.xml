<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.irene.spring.users.dto.UsersDto" 
			alias="usersDto"/>	
		<typeAlias type="com.irene.spring.file.dto.FileDto" 
			alias="fileDto"/>
	</typeAliases>
	<mappers>
		<mapper resource="com/irene/spring/mybatis/UsersMapper.xml"/>
		<mapper resource="com/irene/spring/mybatis/FileMapper.xml"/>
	</mappers>
</configuration>
